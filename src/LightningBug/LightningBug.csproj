<Project Sdk="Microsoft.NET.Sdk">
    
    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <LangVersion>7.3</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.3.3</Version>
        <AssemblyVersion>1.3.3</AssemblyVersion>
        <FileVersion>1.3.3</FileVersion>
        <PackageVersion>1.3.3</PackageVersion>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PackageIconUrl>http://lightningbug.jasondentler.com/images/lightningbug_vector.png</PackageIconUrl>
        <PackageProjectUrl>http://lightningbug.jasondentler.com</PackageProjectUrl>
        <RepositoryUrl>https://github.com/jasondentler/lightning-bug</RepositoryUrl>
        <Description>Lightning Bug is a collection of flashy but short-lived bits of code for common tasks.</Description>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <PackageTags>SQL data bug lightning bulk copy</PackageTags>
        <Authors>Jason Dentler</Authors>
        <Product>Lightning Bug</Product>
        <Title>Lightning Bug</Title>
        <Copyright>Copyright Â© 2019 Jason Dentler</Copyright>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.SqlServer.SqlManagementObjects" Version="140.17283.0" />
      <PackageReference Include="System.Data.SqlClient" Version="4.6.0" />
    </ItemGroup>

</Project>
